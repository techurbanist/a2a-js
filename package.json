{
  "name": "a2a-js",
  "version": "0.2.0",
  "description": "JavaScript SDK for the Agent2Agent (A2A) Protocol - enabling communication between AI agents",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "dev": "tsc --watch",
    "example:server": "node dist/examples/helloworld/server.js",
    "example:client": "node dist/examples/helloworld/client.js",
    "clean": "rimraf dist",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "a2a",
    "agent2agent",
    "ai",
    "agents",
    "protocol",
    "interoperability",
    "sdk"
  ],
  "author": "techurbanist",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techurbanist/a2a-js.git"
  },
  "bugs": {
    "url": "https://github.com/techurbanist/a2a-js/issues"
  },
  "homepage": "https://github.com/techurbanist/a2a-js#readme",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.18",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "eventsource": "^3.0.7",
    "express": "^5.1.0",
    "jsonschema": "^1.5.0",
    "uuid": "^11.1.0"
  }
}
